# Select backend: openai or ollama
BACKEND=ollama

# Ollama settings
OLLAMA_HOST=http://localhost:11434
OLLAMA_MODEL=llama3.1

# OpenAI settings (only used when BACKEND=openai)
OPENAI_API_KEY=sk-...
OPENAI_MODEL=gpt-5
OPENAI_TEMPERATURE=0.2
OPENAI_SEED=42

# Target style file
TARGET_STYLE=prompts/targets/default.md
